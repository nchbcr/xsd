					<xs:enumeration value="Cervix"/>
					<xs:enumeration value="Chest"/>
					<xs:enumeration value="Chest wall"/>
					<xs:enumeration value="Chest/Abdomen/Pelvis"/>
					<xs:enumeration value="Chest/Pelvis"/>
