<?xml version="1.0" encoding="utf-8"?>

<xs:schema 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:utility="http://tcga.nci/bcr/xml/utility/2.7" 
	xmlns:admin="http://tcga.nci/bcr/xml/administration/2.7" 
	xmlns:shared="http://tcga.nci/bcr/xml/shared/2.7"
	xmlns:clin_shared="http://tcga.nci/bcr/xml/clinical/shared/2.7"
	xmlns:ssf_disease="http://tcga.nci/bcr/xml/ssf_disease/2.7"
	xmlns="http://tcga.nci/bcr/xml/ssf/2.7" 
	targetNamespace="http://tcga.nci/bcr/xml/ssf/2.7" 
	elementFormDefault="qualified" 
	version="2.7.16">
	
	<xs:import namespace="http://tcga.nci/bcr/xml/utility/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/utility/2.7/TCGA_BCR.Utility.xsd"/>
	<xs:import namespace="http://tcga.nci/bcr/xml/administration/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/administration/2.7/TCGA_BCR.Administration.xsd"/>
	<xs:import namespace="http://tcga.nci/bcr/xml/shared/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/shared/2.7/TCGA_BCR.Shared_Elements.xsd"/>
	<xs:import namespace="http://tcga.nci/bcr/xml/clinical/shared/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/shared/2.7/TCGA_BCR.Shared_Clinical_Elements.xsd" />
	<xs:import namespace="http://tcga.nci/bcr/xml/ssf_disease/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/ssf/2.7/TCGA_BCR.SSF_Disease_Details.xsd"/>
	
	<xs:annotation>
		<xs:documentation xml:lang="en">Schema to define elements of the Sample Submission Form.</xs:documentation>
	</xs:annotation>
	
	<xs:element name="tcga_bcr">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="admin:admin"/>
				<xs:element ref="patient"/>
			</xs:sequence>
			<xs:attribute name="schemaVersion" type="xs:decimal" use="required" fixed="2.7"/>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="patient">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="admin:additional_studies"/>
				<xs:element ref="shared:bcr_patient_barcode"/>
				<xs:element ref="shared:bcr_patient_uuid"/>
				<xs:element ref="shared:tissue_source_site"/>
				<xs:element ref="shared:patient_id"/>
				<xs:element ref="shared:hiv_status" minOccurs="0"/>
				<xs:element ref="tumor_samples"/>
				<xs:element ref="normal_controls"/>
				<xs:element ref="patient_details" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="tumor_samples">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="tumor_sample" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="tumor_sample">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="bcr_sample_uuid"/>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="shared:day_of_sample_procurement"/>
						<xs:element ref="shared:month_of_sample_procurement"/>
						<xs:element ref="shared:year_of_sample_procurement"/>
					</xs:sequence>
					<xs:element ref="shared:days_to_sample_procurement"/>
				</xs:choice>
				<xs:element ref="shared:method_of_sample_procurement"/>
				<xs:element ref="shared:other_method_of_sample_procurement"/>
				<xs:element ref="vessel_used" minOccurs="0"/>
				<xs:element ref="other_vessel_used" minOccurs="0"/>
				<xs:element ref="shared:maximum_tumor_dimension" minOccurs="0"/>
				<xs:element ref="tumor_weight" minOccurs="0"/>
				<xs:element ref="sample_prescreened" minOccurs="0"/>
				<xs:element ref="shared:tumor_nuclei_percent" minOccurs="0"/>
				<xs:element ref="shared:tumor_necrosis_percent" minOccurs="0"/>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="day_of_pathology_review" minOccurs="0"/>
						<xs:element ref="month_of_pathology_review" minOccurs="0"/>
						<xs:element ref="year_of_pathology_review" minOccurs="0"/>
					</xs:sequence>
					<xs:element ref="days_to_pathology_review" minOccurs="0"/>
				</xs:choice>
				<xs:element ref="path_confirm_tumor_nuclei_metrics" minOccurs="0"/>
				<xs:element ref="path_confirm_tumor_necrosis_metrics" minOccurs="0"/>
				<xs:element ref="path_confirm_report_attached" minOccurs="0"/>
				<xs:element ref="path_confirm_diagnosis_matching" minOccurs="0"/>
				<xs:element ref="reason_path_confirm_diagnosis_not_matching" minOccurs="0"/>
				<xs:element ref="top_slide_submitted" minOccurs="0"/>
				<xs:element ref="digital_image_submitted" minOccurs="0"/>
				<xs:element ref="ffpe_tumor_slide_submitted" minOccurs="0"/>
				<xs:element ref="submitted_for_lce" minOccurs="0"/>
				<xs:element ref="shared:other_dx"/>
				<xs:element ref="shared:history_of_neoadjuvant_treatment"/>
				<xs:element ref="shared:consent_or_death_status"/>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="shared:day_of_consent"/>
						<xs:element ref="shared:month_of_consent"/>
						<xs:element ref="shared:year_of_consent"/>
					</xs:sequence>
					<xs:element ref="shared:days_to_consent"/>
				</xs:choice>
				<xs:element ref="shared:country" minOccurs="0"/>
				<xs:element ref="ssf_disease:disease_details" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="tumor_histologies"/>
				<xs:element ref="tumor_locations"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="tumor_locations">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="tumor_focality" minOccurs="0"/>
				<xs:element ref="laterality" minOccurs="0"/>
				<xs:element ref="tumor_location" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="tumor_location">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="site_of_disease"/>
				<xs:element ref="site_of_disease_text"/>
				<xs:element ref="site_of_disease_description" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="tumor_histologies">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="tumor_histology" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="tumor_histology">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="shared:histological_type"/>
				<xs:element ref="shared:histological_type_other" minOccurs="0"/>
				<xs:element ref="shared:histological_percentage" minOccurs="0"/>
				<xs:element ref="shared:tumor_morphology_percentage" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="bcr_sample_uuid">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attributeGroup ref="utility:common_ext_attribute_group"/>
					<xs:attribute name="cde" type="xs:string" default=""/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.3"/>
					<xs:attribute name="tier" type="xs:string" default="2"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="tumor_weight">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:extension base="utility:number_or_null">
					<xs:attributeGroup ref="utility:common_ext_attribute_group"/>
					<xs:attribute name="cde" type="xs:string" default="3081946"/>
					<xs:attribute name="units" type="xs:string" fixed="mg"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.5"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="patient_details">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="shared:gender" minOccurs="0"/>
				<xs:choice minOccurs="0">
					<xs:sequence>
						<xs:element ref="clin_shared:day_of_birth"/>
						<xs:element ref="clin_shared:month_of_birth"/>
						<xs:element ref="clin_shared:year_of_birth"/>
					</xs:sequence>
					<xs:element ref="clin_shared:days_to_birth"/>
				</xs:choice>
				<xs:element ref="clin_shared:race_list" minOccurs="0"/>
				<xs:element ref="clin_shared:ethnicity" minOccurs="0"/>
				<xs:element ref="shared:other_dx" minOccurs="0"/>
				<xs:element ref="shared:other_dx_site" minOccurs="0"/>
				<xs:element ref="clin_shared:vital_status" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="normal_controls">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="normal_control" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="normal_control">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="bcr_sample_uuid"/>
				<xs:element ref="method_of_normal_sample_procurement"/>
				<xs:element ref="other_method_of_normal_sample_procurement"/>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="day_of_normal_sample_procurement"/>
						<xs:element ref="month_of_normal_sample_procurement"/>
						<xs:element ref="year_of_normal_sample_procurement"/>
					</xs:sequence>
					<xs:element ref="days_to_normal_sample_procurement"/>
				</xs:choice>
				<xs:element ref="normal_sample_site_proximity_to_tumor"/>
				<xs:element ref="ncedna_dna_conc"/>
				<xs:element ref="ncedna_dna_qm"/>
				<xs:element ref="ncedna_dna_qty"/>
				<xs:element ref="ncedna_dna_vol"/>
				<xs:element ref="normal_control_locations"/>
				<xs:element ref="normal_details" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="method_of_normal_sample_procurement">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:restriction base="utility:clinical_res_attributes">
					<xs:enumeration value=""/>
					<xs:enumeration value="Abdomino-perineal Resection of Rectum"/>
					<xs:enumeration value="Anterior Resection of Rectum"/>
					<xs:enumeration value="Blood Draw"/>
					<xs:enumeration value="Bone Marrow Aspirate"/>
					<xs:enumeration value="Buccal Swab"/>
					<xs:enumeration value="Core Biopsy"/>
					<xs:enumeration value="Cytology"/>
					<xs:enumeration value="Endo Rectal Tumor Resection"/>
					<xs:enumeration value="Excisional Biopsy"/>
					<xs:enumeration value="Fine Needle Aspiration Biopsy"/>
					<xs:enumeration value="Hand Assisted Laparoscopic Radical Nephrectomy"/>
					<xs:enumeration value="Incisional Biopsy"/>
					<xs:enumeration value="Laparoscopic Partial Nephrectomy"/>
					<xs:enumeration value="Laparoscopic Radical Nephrectomy"/>
					<xs:enumeration value="Laparoscopic Radical Prostatectomy with Robotics"/>   
					<xs:enumeration value="Left Hemicolectomy"/>
					<xs:enumeration value="Mouthwash"/>
					<xs:enumeration value="Open Partial Nephrectomy"/>
					<xs:enumeration value="Open Radical Nephrectomy"/>
					<xs:enumeration value="Open Radical Prostatectomy"/>
					<xs:enumeration value="Other Method"/>
					<xs:enumeration value="Right Hemicolectomy"/>
					<xs:enumeration value="Sigmoid Colectomy"/>
					<xs:enumeration value="Skin Punch"/>
					<xs:enumeration value="Surgical Excision specimen"/>
					<xs:enumeration value="Surgical Resection"/>
					<xs:enumeration value="Total Colectomy"/>
					<xs:enumeration value="Transverse Colectomy"/>
					<xs:enumeration value="Tumor Resection"/>
					<xs:attribute name="cde" type="xs:string" default="3288147"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.5"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="other_method_of_normal_sample_procurement">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:restriction base="utility:clinical_res_attributes">
					<xs:attribute name="cde" type="xs:string" default="3288151"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.5"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="day_of_normal_sample_procurement">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:extension base="utility:generic_day">
					<xs:attributeGroup ref="utility:common_ext_attribute_group"/>
					<xs:attribute name="cde" type="xs:string" default="3288196"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.3"/>
					<xs:attribute name="tier" type="xs:string" default="2"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="month_of_normal_sample_procurement">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:extension base="utility:generic_month">
					<xs:attributeGroup ref="utility:common_ext_attribute_group"/>
					<xs:attribute name="cde" type="xs:string" default="3288195"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.3"/>
					<xs:attribute name="tier" type="xs:string" default="2"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="year_of_normal_sample_procurement">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:extension base="utility:generic_year">
					<xs:attributeGroup ref="utility:common_ext_attribute_group"/>
					<xs:attribute name="cde" type="xs:string" default="3288197"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.3"/>
					<xs:attribute name="tier" type="xs:string" default="2"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="days_to_normal_sample_procurement" default="" nillable="true">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:restriction base="utility:clinical_res_attributes">
					<xs:attribute name="cde" type="xs:string" default="3288496"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.5"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="normal_sample_site_proximity_to_tumor">
		<xs:complexType>
			<xs:simpleContent>
				<xs:restriction base="utility:clinical_res_attributes">
					<xs:enumeration value=""/>
					<xs:enumeration value="Adjacent (&lt; or = 2cm)"/>
					<xs:enumeration value="Distal (&gt;2cm)"/>
					<xs:enumeration value="Unknown"/>
					<xs:attribute name="cde" type="xs:string" default="3088708"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.5"/>
					<xs:attribute name="tier" type="xs:string" default="2"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ncedna_dna_conc">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:extension base="utility:number_or_null">
					<xs:attributeGroup ref="utility:common_ext_attribute_group"/>
					<xs:attribute name="cde" type="xs:string" default="3288187"/>
					<xs:attribute name="units" type="xs:string" fixed="ug/ul"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.5"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="ncedna_dna_qm">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:restriction base="utility:clinical_res_attributes">
					<xs:attribute name="cde" type="xs:string" default="3288186"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.5"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="ncedna_dna_qty">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:extension base="utility:number_or_null">
					<xs:attributeGroup ref="utility:common_ext_attribute_group"/>
					<xs:attribute name="cde" type="xs:string" default="3288185"/>
					<xs:attribute name="units" type="xs:string" fixed="ug"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.5"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="ncedna_dna_vol">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:extension base="utility:number_or_null">
					<xs:attributeGroup ref="utility:common_ext_attribute_group"/>
					<xs:attribute name="cde" type="xs:string" default="3288188"/>
					<xs:attribute name="units" type="xs:string" fixed="ul"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.5"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="normal_control_locations">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="normal_control_location" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="normal_control_location">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="normal_tissue_anatomic_site"/>
				<xs:element ref="other_anatomic_site_normal_tissue"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="normal_details">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="shared:country" minOccurs="0"/>
				<xs:element ref="vessel_used" minOccurs="0"/>
				<xs:element ref="other_vessel_used" minOccurs="0"/>
				<xs:element ref="tumor_weight" minOccurs="0"/>
				<xs:element ref="sample_prescreened" minOccurs="0"/>
				<xs:element ref="top_slide_submitted" minOccurs="0"/>
				<xs:element ref="digital_image_submitted" minOccurs="0"/>
				<xs:element ref="surgery_reason" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
