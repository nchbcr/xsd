					<xs:enumeration value="Antrum"/>
					<xs:enumeration value="Anus"/>
					<xs:enumeration value="Aorta"/>
					<xs:enumeration value="Aortic Body"/>
					<xs:enumeration value="Appendix"/>
					<xs:enumeration value="Aqueous Fluid"/>
					<xs:enumeration value="Arm"/>
					<xs:enumeration value="Artery"/>
					<xs:enumeration value="Ascending Colon Hepatic Flexure"/>
					<xs:enumeration value="Ascending colon"/>
					<xs:enumeration value="Ascites"/>
					<xs:enumeration value="Auditory canal"/>
					<xs:enumeration value="Autonomic nervous system"/>
					<xs:enumeration value="Axilla"/>
					<xs:enumeration value="Back"/>
					<xs:enumeration value="Base of Tongue"/>
					<xs:enumeration value="Bile duct"/>
					<xs:enumeration value="Bladder"/>
					<xs:enumeration value="Bladder; Dome"/>
					<xs:enumeration value="Bladder; macroscopic urothelial tissue"/>
					<xs:enumeration value="Blood"/>
					<xs:enumeration value="Blood vessel"/>
					<xs:enumeration value="Bone"/>
					<xs:enumeration value="Bone marrow"/>
					<xs:enumeration value="Bowel"/>
					<xs:enumeration value="Brain"/>
					<xs:enumeration value="Brain stem"/>
					<xs:enumeration value="Brain; Supratentorial"/>
					<xs:enumeration value="Breast"/>
					<xs:enumeration value="Broad ligament"/>
					<xs:enumeration value="Bronchiole"/>
