<?xml version="1.0" encoding="utf-8" ?>

<xs:schema elementFormDefault="qualified" version="2.7.17" 
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:utility="http://tcga.nci/bcr/xml/utility/2.7"
  xmlns:admin="http://tcga.nci/bcr/xml/administration/2.7"
  xmlns:shared="http://tcga.nci/bcr/xml/shared/2.7"
  xmlns:clin_shared="http://tcga.nci/bcr/xml/clinical/shared/2.7"
  xmlns:shared_stage="http://tcga.nci/bcr/xml/clinical/shared/stage/2.7"
  xmlns:rad="http://tcga.nci/bcr/xml/clinical/radiation/2.7"
  xmlns:rx="http://tcga.nci/bcr/xml/clinical/pharmaceutical/2.7"
  xmlns:tx="http://tcga.nci/bcr/xml/clinical/treatment/2.7"
  xmlns:ocg_shared="http://tcga.nci/bcr/xml/clinical/shared/ocg/2.7"
  xmlns:cesc_shared="http://tcga.nci/bcr/xml/clinical/cesc/shared/2.7"
  xmlns:cesc_nte="http://tcga.nci/bcr/xml/clinical/cesc/shared/new_tumor_event/2.7/1.0" 
  xmlns:follow_up_v2.0="http://tcga.nci/bcr/xml/clinical/cesc/followup/2.7/2.0"
  xmlns:follow_up_v4.0="http://tcga.nci/bcr/xml/clinical/cesc/followup/2.7/4.0"
  xmlns="http://tcga.nci/bcr/xml/clinical/cesc/2.7"
  targetNamespace="http://tcga.nci/bcr/xml/clinical/cesc/2.7">
      
    <xs:annotation>
        <xs:documentation xml:lang="en">Schema to define the elements of the CESC study.</xs:documentation>
    </xs:annotation>
  
    <xs:import namespace="http://tcga.nci/bcr/xml/utility/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/utility/2.7/TCGA_BCR.Utility.xsd" />
    <xs:import namespace="http://tcga.nci/bcr/xml/administration/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/administration/2.7/TCGA_BCR.Administration.xsd" />
	<xs:import namespace="http://tcga.nci/bcr/xml/shared/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/shared/2.7/TCGA_BCR.Shared_Elements.xsd" />
    <xs:import namespace="http://tcga.nci/bcr/xml/clinical/shared/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/shared/2.7/TCGA_BCR.Shared_Clinical_Elements.xsd" />
    <xs:import namespace="http://tcga.nci/bcr/xml/clinical/shared/stage/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/shared/2.7/TCGA_BCR.Stage_Clinical_Elements.xsd" />
	<xs:import namespace="http://tcga.nci/bcr/xml/clinical/radiation/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/radiation/2.7/TCGA_BCR.Radiation.xsd" />
    <xs:import namespace="http://tcga.nci/bcr/xml/clinical/pharmaceutical/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/pharmaceutical/2.7/TCGA_BCR.Pharmaceutical.xsd" />
	<xs:import namespace="http://tcga.nci/bcr/xml/clinical/shared/ocg/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/shared/2.7/TCGA_BCR.OCG_Shared_Clinical_Elements.xsd" />
	<xs:import namespace="http://tcga.nci/bcr/xml/clinical/cesc/shared/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/cesc/shared/2.7/TCGA_BCR.CESC_Clinical_Shared_Datatypes.xsd" />
	<xs:import namespace="http://tcga.nci/bcr/xml/clinical/cesc/shared/new_tumor_event/2.7/1.0" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/cesc/shared/2.7/TCGA_BCR.CESC_Clinical_NTEs_v1.0.xsd" />
	<xs:import namespace="http://tcga.nci/bcr/xml/clinical/cesc/followup/2.7/2.0" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/cesc/followup/2.7/TCGA_BCR.CESC_Clinical_FollowUp_v2.0.xsd" />
    <xs:import namespace="http://tcga.nci/bcr/xml/clinical/cesc/followup/2.7/4.0" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/cesc/followup/2.7/TCGA_BCR.CESC_Clinical_FollowUp_v4.0.xsd" />
    <xs:import namespace="http://tcga.nci/bcr/xml/clinical/treatment/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/clinical/treatment/2.7/TCGA_BCR.Treatment.xsd" />

    <xs:element name="tcga_bcr">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="admin:admin" />
                <xs:element ref="patient" />
            </xs:sequence>
            
            <xs:attribute name="schemaVersion" type="xs:decimal" use="required" fixed="2.7"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="patient">
        <xs:complexType>
            <xs:sequence>
				<xs:element ref="admin:additional_studies" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="shared:tissue_source_site" />
                <xs:element ref="shared:patient_id" />
				<xs:element ref="shared:bcr_patient_barcode" />
                <xs:element ref="shared:bcr_patient_uuid" />
				<xs:element ref="clin_shared:informed_consent_verified" minOccurs= "0"/>  
                <xs:element ref="clin_shared:icd_o_3_site" />
                <xs:element ref="clin_shared:icd_o_3_histology" />
                <xs:element ref="clin_shared:icd_10" />
				
				<xs:element ref="clin_shared:tissue_prospective_collection_indicator" />
				<xs:element ref="clin_shared:tissue_retrospective_collection_indicator" />
				<xs:choice>
                    <xs:sequence>
                        <xs:element ref="clin_shared:day_of_birth" />
                        <xs:element ref="clin_shared:month_of_birth" />
                        <xs:element ref="clin_shared:year_of_birth" />
                    </xs:sequence>
                    <xs:element ref="clin_shared:days_to_birth" />
                </xs:choice>
				<xs:element ref="shared:gender" />
				<xs:element ref="clin_shared:menopause_status" />
				<xs:element ref="clin_shared:height" />
                <xs:element ref="clin_shared:weight" />
                <xs:element ref="clin_shared:race_list"/>
				<xs:element ref="clin_shared:ethnicity" />
				<xs:element ref="shared:other_dx" />
				<xs:element ref="shared:history_of_neoadjuvant_treatment" />
                <xs:element ref="clin_shared:vital_status" />
                <xs:choice>
                    <xs:sequence>
                        <xs:element ref="clin_shared:day_of_last_followup" />
                        <xs:element ref="clin_shared:month_of_last_followup" />
                        <xs:element ref="clin_shared:year_of_last_followup" />
                    </xs:sequence>
                    <xs:element ref="clin_shared:days_to_last_followup" />
                </xs:choice>
                <xs:choice minOccurs="0">
                    <xs:sequence>
                        <xs:element ref="clin_shared:day_of_last_known_alive" />
                        <xs:element ref="clin_shared:month_of_last_known_alive" />
                        <xs:element ref="clin_shared:year_of_last_known_alive" />
                    </xs:sequence>
                    <xs:element ref="clin_shared:days_to_last_known_alive" />
                </xs:choice>
                <xs:choice>
                    <xs:sequence>
                        <xs:element ref="clin_shared:day_of_death" />
                        <xs:element ref="clin_shared:month_of_death" />
                        <xs:element ref="clin_shared:year_of_death" />
                    </xs:sequence>
                    <xs:element ref="clin_shared:days_to_death" />
                </xs:choice>
				<xs:element ref="clin_shared:person_neoplasm_cancer_status" />
                <xs:element ref="clin_shared:birth_control_pill_history_usage_category" />
				<xs:element ref="number_of_successful_pregnancies_which_resulted_in_at_least_1_live_birth" />
                <xs:element ref="live_birth_number" minOccurs="0" />
				<xs:element ref="patient_pregnancy_spontaneous_abortion_count" />
				<xs:element ref="patient_pregnancy_therapeutic_abortion_count" />
				<xs:element ref="ectopic_pregnancy_count" />
				<xs:element ref="pregnancy_stillbirth_count" />
				<xs:element ref="female_breast_feeding_or_pregnancy_status_indicator" minOccurs = "0"/>
                <xs:element ref="total_number_of_pregnancies" />
                <xs:element ref="patient_pregnancy_count" minOccurs="0" />
				<xs:element ref="shared:tobacco_smoking_history" />
                <xs:element ref="clin_shared:age_began_smoking_in_years" />
                <xs:element ref="clin_shared:stopped_smoking_year" /> 
				<xs:element ref="clin_shared:number_pack_years_smoked" />
                <xs:element ref="patient_history_immune_system_and_related_disorders_names" minOccurs = "0"/>
                <xs:element ref="patient_history_immune_system_and_related_disorders_text" minOccurs = "0"/>   
                <xs:element ref="clin_shared:eastern_cancer_oncology_group" />
                <xs:element ref="clin_shared:karnofsky_performance_score" minOccurs = "0" />
                <xs:element ref="clin_shared:performance_status_scale_timing" minOccurs= "0" />
                <xs:element ref="clin_shared:assessment_timepoint_category" minOccurs = "0" />
                <xs:choice minOccurs = "0">
                    <xs:sequence>
                        <xs:element ref="clin_shared:day_of_performance_status_assessment" />
                        <xs:element ref="clin_shared:month_of_performance_status_assessment" />
                        <xs:element ref="clin_shared:year_of_performance_status_assessment" />
                    </xs:sequence>
                    <xs:element ref="clin_shared:days_to_performance_status_assessment"  />
                </xs:choice>

                <xs:element ref="history_of_disease"  minOccurs="0" maxOccurs="1" />

                <xs:element ref="tumor_response_cdus_type" minOccurs="0" />
				
				<xs:element ref="clin_shared:primary_therapy_outcome_success" />
                <xs:element ref="clin_shared:radiation_therapy" minOccurs="0" />
                <xs:element ref="clin_shared:postoperative_rx_tx" minOccurs="0" />

				<xs:element ref="clin_shared:tumor_tissue_site" />
				<xs:element ref="shared:histological_type"/>
				<xs:element ref="shared:histological_type_other" minOccurs="0" />
				<xs:element ref="keratinizing_squamous_cell_carcinoma_present_indicator" />
				<xs:element ref="shared:neoplasm_histologic_grade" />
				
				<xs:choice>
                    <xs:sequence>
                        <xs:element ref="clin_shared:day_of_initial_pathologic_diagnosis" />
                        <xs:element ref="clin_shared:month_of_initial_pathologic_diagnosis" />
                    </xs:sequence>
                    <xs:sequence>
                        <xs:element ref="clin_shared:days_to_initial_pathologic_diagnosis" />
                        <xs:element ref="clin_shared:age_at_initial_pathologic_diagnosis" />
                    </xs:sequence>
                </xs:choice>
                <xs:element ref="clin_shared:year_of_initial_pathologic_diagnosis" />
             
                <xs:element ref="clin_shared:initial_pathologic_diagnosis_method" />
                <xs:element ref="clin_shared:init_pathology_dx_method_other" />
                <xs:element ref="lymph_node_location_positive_pathology_names" />
                <xs:element ref="lymph_node_location_positive_pathology_text" />
                <xs:choice minOccurs="0" >
                    <xs:sequence>
                        <xs:element ref="shared:day_of_sample_procurement" />
                        <xs:element ref="shared:month_of_sample_procurement" />
                        <xs:element ref="shared:year_of_sample_procurement" />
                    </xs:sequence>
                    <xs:element ref="shared:days_to_sample_procurement" />
                </xs:choice>
                <xs:element ref="hysterectomy_Performed_Ind-3" minOccurs="0" />
                <xs:element ref="hysterectomy_performed_type" />
                <xs:element ref="hysterectomy_performed_text" />
                <xs:element ref="cervical_neoplasm_pathologic_margin_involved_type" />
                <xs:element ref="cervical_neoplasm_pathologic_margin_involved_text" />
                <xs:element ref="cervical_carcinoma_pelvic_extension_text" />

				<xs:element ref="clin_shared:primary_lymph_node_presentation_assessment" />
				<xs:element ref="clin_shared:lymph_node_examined_count" />  
                <xs:element ref="clin_shared:number_of_lymphnodes_positive_by_he" />
                <xs:element ref="clin_shared:number_of_lymphnodes_positive_by_ihc" />
                <xs:element ref="lymphovascular_invasion_indicator" />
                <xs:element ref="cervical_carcinoma_corpus_uteri_involvement_indicator" />

                <xs:choice>
                    <xs:sequence>
                        <xs:element ref="day_of_fdg_or_ct_pet_performed" />
                        <xs:element ref="month_of_fdg_or_ct_pet_performed" />
                        <xs:element ref="year_of_fdg_or_ct_pet_performed" />
                    </xs:sequence>
                    <xs:element ref="days_to_fdg_or_ct_pet_performed" />
                </xs:choice>
                <xs:element ref="standardized_uptake_value_cervix_uteri_assessment_measurement" minOccurs = "0"/>
				<xs:element ref="fdg_or_ct_pet_performed_outcomes" />
				<xs:choice>
                    <xs:sequence>
                        <xs:element ref="day_of_diagnostic_mri_performed" />
                        <xs:element ref="month_of_diagnostic_mri_performed" />
                        <xs:element ref="year_of_diagnostic_mri_performed" />
                    </xs:sequence>
                    <xs:element ref="days_to_diagnostic_mri_performed" />
                </xs:choice>
				<xs:element ref="diagnostic_mri_result_outcomes" />
				<xs:choice>
                    <xs:sequence>
                        <xs:element ref="day_of_diagnostic_computed_tomography_performed" />
                        <xs:element ref="month_of_diagnostic_computed_tomography_performed" />
                        <xs:element ref="year_of_diagnostic_computed_tomography_performed" /> 
                    </xs:sequence>
                    <xs:element ref="days_to_diagnostic_computed_tomography_performed" />
                </xs:choice>
				<xs:element ref="diagnostic_ct_result_outcomes" />
				<xs:element ref="human_papillomavirus_types" />
                <xs:element ref="human_papillomavirus_other_type_text" />
				<xs:element ref="human_papillomavirus_laboratory_procedure_performed_name" />
                <xs:element ref="human_papillomavirus_laboratory_procedure_performed_text" />
                <xs:element ref="oligonucleotide_primer_pair_laboratory_procedure_performed_name" />
                <xs:element ref="oligonucleotide_primer_pair_laboratory_procedure_performed_text" />
				<xs:element ref="laboratory_procedure_tumor_marker_squamous_cell_carcinoma_antigen_result_value" />
				<xs:choice>
                    <xs:sequence>
                        <xs:element ref="day_of_laboratory_procedure_tumor_marker_squamous_cell_carcinoma_antigen_result" />
                        <xs:element ref="month_of_laboratory_procedure_tumor_marker_squamous_cell_carcinoma_antigen_result" />
                        <xs:element ref="year_of_laboratory_procedure_tumor_marker_squamous_cell_carcinoma_antigen_result" />
                    </xs:sequence>
                    <xs:element ref="days_to_laboratory_procedure_tumor_marker_squamous_cell_carcinoma_antigen_result" />
                </xs:choice>

				<xs:element ref="shared_stage:stage_event" />
				
				<!-- The following was added on the v4 enrollment form. -->
				<!-- Find the order in which these should go into this XSD -->
				<xs:element ref="clin_shared:patient_death_reason" />
				<xs:element ref="cesc_shared:death_cause_text" />
				<xs:element ref="cesc_shared:pregnant_at_diagnosis" />
				<xs:element ref="cesc_shared:treatment" />
				<xs:element ref="cesc_nte:new_tumor_events" minOccurs="1" maxOccurs="1" />
				
				<xs:choice>
                    <xs:sequence>
                        <xs:element ref="clin_shared:day_of_form_completion" /> 
                        <xs:element ref="clin_shared:month_of_form_completion" /> 
                        <xs:element ref="clin_shared:year_of_form_completion" /> 
                    </xs:sequence>
                    <xs:element ref="clin_shared:days_to_form_completion" /> 
                </xs:choice> 
				
				<xs:element ref="follow_ups" />
                <xs:element ref="rx:drugs" />
                <xs:element ref="rad:radiations" />
				<xs:element ref="tx:treatments" minOccurs="0" />
				
                <xs:element ref="clin_shared:lost_follow_up" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="history_of_disease" nillable="true">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="shared:hiv_status" />

                <xs:choice>
                    <xs:sequence>
                        <xs:element ref="ocg_shared:day_of_hiv_diagnosis" />
                        <xs:element ref="ocg_shared:month_of_hiv_diagnosis" />
                    </xs:sequence>
                    <xs:element ref="ocg_shared:days_to_hiv_diagnosis" />
                </xs:choice>
				<xs:element ref="ocg_shared:year_of_hiv_diagnosis" minOccurs="0" />

                <xs:element ref="ocg_shared:hbv_test" />
                <xs:element ref="ocg_shared:hcv_test" />
                <xs:element ref="ocg_shared:hpv_test" />
                <xs:element ref="ocg_shared:kshv_hhv8_test" />
                <xs:element ref="ocg_shared:nadir_cd4_counts" />
                <xs:element ref="ocg_shared:hiv_rna_load_at_diagnosis" />
                <xs:element ref="ocg_shared:on_haart_therapy_at_cancer_diagnosis" />
                <xs:element ref="ocg_shared:on_haart_therapy_prior_to_cancer_diagnosis" />
                <xs:element ref="ocg_shared:prior_aids_conditions" />
                <xs:element ref="ocg_shared:cd4_counts_at_diagnosis" />
                <xs:element ref="ocg_shared:cdc_hiv_risk_group" />
                <xs:element ref="ocg_shared:history_of_other_malignancy" />
                <xs:element ref="ocg_shared:history_immunological_disease" />
                <xs:element ref="ocg_shared:history_immunological_disease_other" />
                <xs:element ref="ocg_shared:history_immunosuppresive_rx" />
                <xs:element ref="ocg_shared:history_immunosuppressive_rx_other" />
                <xs:element ref="ocg_shared:history_relevant_infectious_dx" />
                <xs:element ref="ocg_shared:history_relevant_infectious_dx_other" />

            </xs:sequence>
        </xs:complexType>
    </xs:element>


    <xs:element name="follow_ups">
		<xs:annotation>
			<xs:documentation xml:lang="en">We are using namespaces and version numbers to distinguish one version of followup from another.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
		  <xs:sequence>
			<xs:element ref="follow_up_v2.0:follow_up" minOccurs="0" maxOccurs="unbounded" />
			<xs:element ref="follow_up_v4.0:follow_up" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
	</xs:element>

    <xs:element name="number_of_successful_pregnancies_which_resulted_in_at_least_1_live_birth" nillable="true">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attributeGroup ref="utility:common_ext_attribute_group" />
                    <xs:attribute name="cde" type="xs:string" default="2183299" />
                    <xs:attribute name="xsd_ver" type="xs:string" default="2.3" />
                    <xs:attribute name="tier" type="xs:string" default="2" />
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="live_birth_number" nillable="true">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="utility:number_or_null">
                    <xs:attributeGroup ref="utility:common_ext_attribute_group" />
                    <xs:attribute name="cde" type="xs:string" default="2486644" />
                    <xs:attribute name="xsd_ver" type="xs:string" default="2.6" />
                    <xs:attribute name="tier" type="xs:string" default="2" />
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="patient_pregnancy_spontaneous_abortion_count" nillable="true">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="utility:number_or_null">
                    <xs:attributeGroup ref="utility:common_ext_attribute_group" />
                    <xs:attribute name="cde" type="xs:string" default="2180637" />
                    <xs:attribute name="xsd_ver" type="xs:string" default="2.3" />
                    <xs:attribute name="tier" type="xs:string" default="2" />
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="patient_pregnancy_therapeutic_abortion_count" nillable="true">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="utility:number_or_null">
                    <xs:attributeGroup ref="utility:common_ext_attribute_group" />
                    <xs:attribute name="cde" type="xs:string" default="2180648" />
                    <xs:attribute name="xsd_ver" type="xs:string" default="2.3" />
                    <xs:attribute name="tier" type="xs:string" default="2" />
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="ectopic_pregnancy_count" nillable="true">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="utility:number_or_null">
                    <xs:attributeGroup ref="utility:common_ext_attribute_group" />
                    <xs:attribute name="cde" type="xs:string" default="2261915" />
                    <xs:attribute name="xsd_ver" type="xs:string" default="2.3" />
                    <xs:attribute name="tier" type="xs:string" default="2" />
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="pregnancy_stillbirth_count" nillable="true">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="utility:number_or_null">
                    <xs:attributeGroup ref="utility:common_ext_attribute_group" />
                    <xs:attribute name="cde" type="xs:string" default="2183304" />
                    <xs:attribute name="xsd_ver" type="xs:string" default="2.3" />
                    <xs:attribute name="tier" type="xs:string" default="2" />
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="female_breast_feeding_or_pregnancy_status_indicator" nillable="true">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="utility:yes_or_no">
                    <xs:attributeGroup ref="utility:common_ext_attribute_group" />
                    <xs:attribute name="cde" type="xs:string" default="3012573" />
                    <xs:attribute name="xsd_ver" type="xs:string" default="2.3" />
                    <xs:attribute name="tier" type="xs:string" default="2" />
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>

    <!-- TODO: Remove this XSD in 2.8.0 - DEPRECATED: this is getting replace by 2180638 which is already in use -->
    <xs:element name="total_number_of_pregnancies" nillable="true">
        <xs:annotation>
            <xs:documentation>This element is deprecated, will be removed in XSD 2.8 release.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
