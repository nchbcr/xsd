					<xs:attributeGroup ref="utility:common_ext_attribute_group"/>
					<xs:attribute name="cde" type="xs:string" default="2896956"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.4"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="year_of_index" nillable="true">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:extension base="utility:generic_year">
					<xs:attributeGroup ref="utility:common_ext_attribute_group"/>
					<xs:attribute name="cde" type="xs:string" default="2896960"/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.4"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="days_to_index">
		<xs:complexType mixed="true">
			<xs:simpleContent>
				<xs:restriction base="utility:common_res_attributes">
