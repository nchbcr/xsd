					<xs:enumeration value="Human Tumor Original Cells"/>
					<xs:enumeration value="Primary Blood Derived Cancer - Bone Marrow"/>
					<xs:enumeration value="Cell Lines"/>
					<xs:enumeration value="Xenograft Tissue"/>
					<xs:enumeration value="Bone Marrow Normal"/>
					<xs:enumeration value="FFPE Scrolls"/>
					<xs:enumeration value="Fibroblasts from Bone Marrow Normal"/>
					<xs:enumeration value="Mononuclear Cells from Bone Marrow Normal"/>
					<xs:enumeration value="Pleural Effusion"/>
					<xs:enumeration value="FFPE Recurrent"/>
					<xs:enumeration value="Centroblasts"/>
					<xs:enumeration value="Centrocyte"/>
					<xs:enumeration value="Blood Derived Cancer - Bone Marrow, Post-treatment"/>
					<xs:enumeration value="Lymphoid Normal"/>
					<xs:attribute name="cde" type="xs:string" default=""/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.4"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="longest_dimension">
		<xs:complexType mixed="true">
