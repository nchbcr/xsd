					<xs:enumeration value="Primary Tumor and Recurrent Tumor(s)"/>
					<xs:enumeration value="Metastatic Tumor and Recurrent Tumor(s)"/>
					<xs:enumeration value="Primary Tumor and Additional Primary Tumor(s)"/>
