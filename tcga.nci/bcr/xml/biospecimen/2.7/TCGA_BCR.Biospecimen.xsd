<?xml version="1.0" encoding="utf-8"?>

<xs:schema 
 xmlns:xs="http://www.w3.org/2001/XMLSchema" 
 xmlns:utility="http://tcga.nci/bcr/xml/utility/2.7" 
 xmlns:admin="http://tcga.nci/bcr/xml/administration/2.7" 
 xmlns:shared="http://tcga.nci/bcr/xml/shared/2.7"
 xmlns:bio_shared="http://tcga.nci/bcr/xml/biospecimen/shared/2.7"
 xmlns="http://tcga.nci/bcr/xml/biospecimen/2.7" 
 targetNamespace="http://tcga.nci/bcr/xml/biospecimen/2.7" 
 elementFormDefault="qualified" 
 version="2.7.13">

	<xs:annotation>
		<xs:documentation xml:lang="en">Schema to define the elements of the biospecimen xml.</xs:documentation>
	</xs:annotation>

	<xs:import namespace="http://tcga.nci/bcr/xml/utility/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/utility/2.7/TCGA_BCR.Utility.xsd"/>
	<xs:import namespace="http://tcga.nci/bcr/xml/administration/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/administration/2.7/TCGA_BCR.Administration.xsd"/>
	<xs:import namespace="http://tcga.nci/bcr/xml/shared/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/shared/2.7/TCGA_BCR.Shared_Elements.xsd"/>
	<xs:import namespace="http://tcga.nci/bcr/xml/biospecimen/shared/2.7" schemaLocation="https://raw.githubusercontent.com/nchbcr/xsd/2.7/tcga.nci/bcr/xml/biospecimen/2.7/TCGA_BCR.Shared_Bio_Elements.xsd"/>
	
	<xs:element name="tcga_bcr">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="admin:admin"/>
				<xs:element ref="patient"/>
			</xs:sequence>
			<xs:attribute name="schemaVersion" type="xs:decimal" use="required" fixed="2.7"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="patient">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="shared:notes" minOccurs="0"/>
				<xs:element ref="admin:additional_studies" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="admin:alternate_identifiers" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="shared:bcr_patient_barcode"/>
				<xs:element ref="shared:bcr_patient_uuid"/>
				<xs:element ref="shared:tissue_source_site"/>
				<xs:element ref="shared:patient_id"/>
                <xs:element ref="shared:gender" minOccurs="0" maxOccurs="1"/>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="day_of_index"/>
						<xs:element ref="month_of_index"/>
						<xs:element ref="year_of_index"/>
					</xs:sequence>
					<xs:element ref="days_to_index"/>
				</xs:choice>
				<xs:element ref="bcr_canonical_check"/>
				<xs:element ref="samples"/>
				<xs:element ref="hpv_test_results" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="bcr_canonical_check">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="bcr_patient_canonical_status"/>
				<xs:element ref="bcr_patient_canonical_reasons" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="bcr_patient_canonical_reasons">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="bcr_canonical_reason" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="samples">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="sample" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="sample">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="shared:notes" minOccurs="0"/>
				<xs:element ref="admin:additional_studies" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="admin:alternate_identifiers" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="specimen_fraction_ordinal" minOccurs="0"/>
				<xs:element ref="sample_type_id"/>
				<xs:element ref="vial_number"/>
				<xs:element ref="biospecimen_sequence" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="sample_type"/>
				<xs:element ref="longest_dimension"/>
				<xs:element ref="intermediate_dimension"/>
				<xs:element ref="shortest_dimension"/>
				<xs:element ref="initial_weight"/>
				<xs:element ref="current_weight"/>
				<xs:element ref="freezing_method"/>
				<xs:element ref="oct_embedded"/>
                <xs:element ref="preservation_method" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="tissue_type" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="composition" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="tumor_descriptor" minOccurs="0" maxOccurs="1"/>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="day_of_collection"/>
						<xs:element ref="month_of_collection"/>
						<xs:element ref="year_of_collection"/>
					</xs:sequence>
					<xs:element ref="days_to_collection"/>
				</xs:choice>
				<xs:element ref="time_between_clamping_and_freezing"/>
				<xs:element ref="time_between_excision_and_freezing"/>
				<xs:element ref="bcr_sample_barcode"/>
				<xs:element ref="bcr_sample_uuid"/>
				<xs:element ref="is_ffpe" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="bcr_biospecimen_canonical_reasons" minOccurs="0"/>
				<xs:element ref="portions"/>
				<xs:element ref="tumor_pathology"/>
				<xs:element ref="method_of_sample_procurement"/>
				<xs:element ref="bio_shared:other_method_of_sample_procurement" minOccurs="0"/>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="shared:day_of_sample_procurement"/>
						<xs:element ref="shared:month_of_sample_procurement"/>
						<xs:element ref="shared:year_of_sample_procurement"/>
					</xs:sequence>
					<xs:element ref="shared:days_to_sample_procurement"/>
				</xs:choice>
				<xs:element ref="pathology_report_uuid"/>
				<xs:element ref="pathology_report_file_name" minOccurs="0"/>
				<xs:element ref="diagnostic_slides"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="sample_type_id" nillable="false">
		<xs:complexType>
			<xs:simpleContent>
				<xs:restriction base="utility:common_res_attributes">
					<xs:enumeration value=""/>
					<xs:enumeration value="01"/>
					<xs:enumeration value="02"/>
					<xs:enumeration value="03"/>
					<xs:enumeration value="04"/>
					<xs:enumeration value="05"/>
					<xs:enumeration value="06"/>
					<xs:enumeration value="07"/>
					<xs:enumeration value="08"/>
					<xs:enumeration value="09"/>
					<xs:enumeration value="10"/>
					<xs:enumeration value="11"/>
					<xs:enumeration value="12"/>
					<xs:enumeration value="13"/>
					<xs:enumeration value="14"/>
					<xs:enumeration value="15"/>
					<xs:enumeration value="16"/>
					<xs:enumeration value="17"/>
					<xs:enumeration value="20"/>
					<xs:enumeration value="41"/>
					<xs:enumeration value="50"/>
					<xs:enumeration value="60"/>
					<xs:enumeration value="61"/>
					<xs:enumeration value="99"/>
					<xs:attribute name="cde" type="xs:string" default=""/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.4"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="vial_number" nillable="false">
		<xs:complexType>
			<xs:simpleContent>
				<xs:restriction base="utility:common_res_attributes">
					<xs:length value="1"/>
					<xs:attribute name="cde" type="xs:string" default=""/>
					<xs:attribute name="xsd_ver" type="xs:string" default="2.4"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
    <xs:element name="biospecimen_sequence" nillable="true">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="utility:integer_or_null">
                    <xs:attributeGroup ref="utility:common_ext_attribute_group"/>
                    <xs:attribute name="cde" type="xs:string" default=""/>
                    <xs:attribute name="xsd_ver" type="xs:string" default="2.6"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
	<xs:element name="sample_type">
		<xs:complexType>
			<xs:simpleContent>
				<xs:restriction base="utility:common_res_attributes">
					<xs:enumeration value=""/>
					<xs:enumeration value="Blood Derived Normal"/>
					<xs:enumeration value="Buccal Cell Normal"/>
					<xs:enumeration value="Control Analyte"/>
					<xs:enumeration value="DNA"/>
					<xs:enumeration value="EBV Immortalized Normal"/>
					<xs:enumeration value="GenomePlex (Rubicon) Amplified DNA"/>
