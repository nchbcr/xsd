					<xs:attribute name="xsd_ver" type="xs:string" default="1.8"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="shortest_dimension">
		<xs:complexType mixed="true">
