					<xs:enumeration value="Normal Tissue as only normal" />
					<xs:enumeration value="Other non-canonical" />
					<xs:enumeration value="Prostate Pilot" />
					<xs:enumeration value="Tumor Adjacent Normal" />
					
					<xs:attribute name="cde" type="xs:string" default=""/>
