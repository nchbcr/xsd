					<xs:enumeration value="Incomplete distribution of specimens" />
					<xs:enumeration value="Metastatic Tumor" />
					<xs:enumeration value="Additional Primary Tumor" />
					<xs:enumeration value="Recurrent Tumor" />
					<xs:enumeration value="Fallopian Tube study" />
